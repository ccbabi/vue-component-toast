{
  "name": "vue-component-toast",
  "version": "1.0.3",
  "description": "Easy to use vue toast component.",
  "main": "dist/vc-toast.js",
  "module": "dist/vc-toast.es.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "predist": "npm run clean",
    "dev": "rollup -c --watch",
    "reload": "livereload dist/ -d",
