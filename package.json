    "dist": "NODE_ENV=production rollup -c",
    "postpublish": "git tag ${npm_package_version} && git push origin master --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ccbabi/vue-component-toast.git"
  },
